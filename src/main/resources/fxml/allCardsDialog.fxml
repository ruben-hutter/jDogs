<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane stylesheets="@stylesheet4.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="jDogs.gui.AllCardsDialogController">
   <center>
      <ImageView fx:id="cardImage" fitHeight="258.0" fitWidth="139.0" onMouseClicked="#setCardImage" pickOnBounds="true" preserveRatio="true" BorderPane.alignment="CENTER" />
   </center>
   <left>
      <Button fx:id="chooseButton" mnemonicParsing="false" onAction="#chooseButtonOnAction" text="Choose" BorderPane.alignment="CENTER" />
   </left>
</BorderPane>
